 # run type = [excitation] energy, with functional = CAMB3LYP, and TDDFT
 $CONTRL SCFTYP=RHF TDDFT=EXCITE DFTTYP=CAMB3LYP RUNTYP=ENERGY                     
       ICHARG=0 MULT=1 COORD=UNIQUE MAXIT=200 $END   
 #TDDFT requires lots of memory space
 # memory requested at each node =1000 million words
 # memory reserved for communication = 1000 million words                                       
 $SYSTEM MWORDS=200 MEMDDI=250 $END
 #activate direct SCF calculation
 $SCF DIRSCF=.T. $END
 # find 5 excited states - the current setting is purely driven by its lower cost
 # Previous experience shows that 10 states gives only a few strong peak.                                                         
 $TDDFT NSTATE=5 TPA=.f. $END   
 # Basis set = 6-311++(2d,p)
 $BASIS GBASIS=N311 NGAUSS=6 NDFUNC=2 NPFUNC=1 
       DIFFSP=.TRUE. DIFFS=.TRUE. POLAR=POPN311 $END     
 # solute geometry - C1 1 = symmetry data        
 $DATA
