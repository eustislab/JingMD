	keywords = {blue shift,bonding,configuration interaction,dispersion interactions,hydrogen,implicit solvent,liquid,nonequilibrium free energy,polarizability,reaction field},
	mendeley-groups = {Honors Thesis writing Again},
	number = {2000},
	pages = {264--280},
	title = {{Two-Response-Time Model Based on CM2/INDO/S2 Electrostatic Potentials for the Dielectric Polarization Component of Solvatochromic Shifts on Vertical Excitation Energies}},
