@article{Conley2008,
	abstract = {The behavior of pharmaceutical compds. in aquatic ecosystems is not well defined.  In order to det. spatial and temporal variations in concns. of pharmaceuticals in the Tennessee River, water samples were collected from multiple points along the river and at the inflow of major tributaries.  Sampling structure was designed to study trends between surface and subsurface samples, seasonal trends (winter, spring, summer, and fall), the direct effect of sewage treatment plants (upstream vs. downstream), and the effect of downstream distance on pharmaceutical concns.  All samples were quantified via solid phase extn. followed by liq. chromatog.-tandem mass spectrometry (LC-MS/MS).  This method yielded reproducible quantitation at low parts per trillion (ng/L) levels for all 14 analytes (acetaminophen, atorvastatin, caffeine, carbamazepine, ciprofloxacin, diltiazem, fluoxetine, levofloxacin, lovastatin, norfluoxetine, ranitidine, sertraline, sulfamethoxazole, and trimethoprim).  Correlation analyses (depth, distance) and repeated-measures ANOVAs (season, sewage treatment plant proximity) were used to det. statistically significant trends for frequently detected pharmaceuticals (caffeine, carbamazepine, sulfamethoxazole).  Caffeine and sulfamethoxazole were found to vary by season in subsurface samples; spring exhibited the highest concns.  Carbamazepine varied in proximity to sewage treatment plant outfall with subsurface samples yielding greater concns. downstream than upstream.  Individual pharmaceuticals displayed pos. correlation between surface and subsurface samples and neg. correlation with downstream distance from the headwaters. [on SciFinder(R)]},
	annote = {CAPLUS AN 2008:1303198(Journal)},
	author = {Conley, Justin M and Symes, Steven J and Schorr, Mark S and Richards, Sean M},
	doi = {10.1016/j.chemosphere.2008.07.062},
	issn = {0045-6535},
	journal = {Chemosphere},
	keywords = {spatial temporal analysis pharmaceutical concn upp},
	mendeley-groups = {Honors Thesis writing Again},
	number = {8},
	pages = {1178--1187},
	publisher = {Elsevier Ltd.},
	title = {{Spatial and temporal analysis of pharmaceutical concentrations in the upper Tennessee River basin.}},
	volume = {73},
	year = {2008}
}
@article{Daneshvar2010,
	abstract = {Purpose In this study, seasonal variations in the concn. profile of four analgesics and one lipid regulator were monitored on their way from a wastewater treatment plant (WWTP) effluent, along a river, and into a lake.  Methods From Dec. 2007 to Dec. 2008, water samples were collected monthly (n = 12) from an upstream point, the effluent, four downstream points of the WWTP, and at the point where the river merges with the lake, and the concns. of ibuprofen, naproxen, bezafibrate, diclofenac, and ketoprofen were detd.  The anal. methodol. involved solid-phase extn. of the target compds. from water samples followed by liq. chromatog. coupled with tandem mass spectrometry for compd. sepn. and detection.  Results The studied pharmaceuticals were found in the effluent at concns. ranging from 31 to 1,852 ng l-1 depending on the season.  In the river and lake, the concns. were much lower (6-400 ng l-1) mainly due to diln. but also to a season-dependent contribution from natural transformation processes.  The mean mass flow of all analgesics was highest during winter while the highest mean mass flow of the lipid regulator bezafibrate was obsd. in spring.  Conclusions The WWTP is the main source of the target compds. in the aquatic environment.  The obsd. winter accumulation signifies the importance of natural transformation processes, which can only be estd. based on mass flow data, on the fate of pharmaceuticals in the environment. [on SciFinder(R)]},
